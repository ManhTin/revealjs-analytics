{
  "name": "nextjs-sandbox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint --write src/",
    "format": "biome format --write src/",
    "prisma:migrate:dev": "dotenv -e .env.local -- pnpm exec prisma migrate dev",
    "prisma:migrate:reset": "dotenv -e .env.local -- pnpm exec prisma migrate reset",
    "prisma:db:push": "dotenv -e .env.local -- pnpm exec prisma db push",
    "prisma:generate": "dotenv -e .env.local -- pnpm exec prisma generate",
    "prisma:studio": "dotenv -e .env.local -- pnpm exec prisma studio"
  },
  "packageManager": "pnpm@10.4.1",
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@hookform/resolvers": "^4.1.0",
    "@prisma/client": "^6.4.0",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.6",
    "@tanstack/react-table": "^8.21.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv-cli": "^8.0.0",
    "lucide-react": "^0.475.0",
    "next": "15.1.6",
    "next-auth": "5.0.0-beta.25",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-day-picker": "8.10.1",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "postcss": "^8",
    "prisma": "^6.4.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
